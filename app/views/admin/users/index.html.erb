<h1>Users</h1>

<table class="table table-hover table-striped">
  <thead>
    <tr>
      <th>Username</th>
      <th>Email</th>
      <th>Admin</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td><%= user.is_doctor %></td>
      </tr>
    <% end %>
  </tbody>
</table>